<!DOCTYPE html>
<html>

  <head>
    <meta charset="utf-8">
    <title>SUBJECT</title>
    <%- include layout/bootstrap %>
  </head>

  <body>
    <%- include layout/header %>
      <ol class="breadcrumb" align=left>
        <li><a href="/homepage">Homepage</a></li>
        <li class="active">
          Subject
        </li>
      </ol>
      <table class="table table-hover">
        <tr>
          <th>NAMA SUBJECT</th>
          <th>TEACHER</th>
          <th>ACTION</th>
        </tr>
        <% dataTcui.forEach( cont => { %>
          <tr>
            <td>
              <%= cont.subject_name %>
            </td>
            <td>
              <% for (let i = 0; i < cont.Teachers.length; i++) { %>
                <%= `${i +1}.` %>
                  <%= cont.Teachers[i].first_name %>
                    <%= cont.Teachers[i].last_name %> <br>
                      <% } %>
            </td>
            <td>
              <a href="/subject/enroll/<%= cont.id %>">Enrolled Students</a>
            </td>
          </tr>
          <% }) %>
      </table>
      <br><br>
      <div align="center">
        <a href="/" class="btn btn-primary btn-lg active">BACK</a
      </div>
  </body>

</html>
